             dead=false},
     moves={atk1={s={8,8},a={10,10},r={12,4},
            ra=14,i=2,d=5},
            atk2={s={10,14},a={20,64},r={25,4}},
            atk3={s={10,32},a={20,32},r={15,32}}}},
    {id=2,
     clr=12,
     roundwins=0,
     activespr=32,
     width=2,
     height=2,
     idlespr=32,
     walkspr={34,32,36,38},
     blockspr=44,
     projectilespr=98,
     drc=true,
     upd,
