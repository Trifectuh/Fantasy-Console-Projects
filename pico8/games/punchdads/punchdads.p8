__lua__
--punchdads: don't get punched and fall off!

#include keys.p8
#include title.p8
#include projectile.p8
#include rend.p8
#include ui.p8
#include attack.p8
#include char.p8

game={
 upd,
 draw
}

function _init()
  cameraoffset=0
  restarttimer=nil
  firstrun=true
  countround=true
  fb={}
  p={{id=1,
     clr=14,
     roundwins=0,
     activespr=0,
     width=2,
     height=2,
     idlespr=0,
     walkspr={2,0,4,6},
